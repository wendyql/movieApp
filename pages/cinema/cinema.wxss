/* pages/cinema/cinema.wxss */
.nav{
  padding: 20rpx 60rpx;
  width: 100%;
  height: 60rpx;
  border-bottom: 4rpx solid #ccc;
  background-color: #f5f5f5;
}
.chengd{
  float: left;
  color: #444;
}
.search{
  float: left;
  border-radius:16rpx; 
  margin-left: 50rpx;
  width: 600rpx;
  border: #ccc solid 1px;
  width: 400rpx;
  padding-left: 32rpx;
  font-size: 30rpx;  
}
.cityList{
  width: 100%;
  height: 270rpx;
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
}
.movieAddress{
  font-weight: bold;
  line-height: 2;
}
.detailAddress{
  width: 640rpx;
  height: 42rpx;
  font-size: 14px;
  color: #666;
  line-height: 1.5;
  overflow: hidden;
}
.tubiao{
  width: 100%;
  height: 70rpx;
  margin-top: 20rpx;
}
.zuo,.gai,.zuo1,.ka,.cuxiao,.distance{
float: left;
}
.zuo,.gai{
  display: inline-block;
  text-align: center;
  border: 1px solid #72A4B1;
  border-radius: 8rpx;
  background-color: #fff;
  color: #72A4B1;
}
.gai{
  margin-left: 20rpx;
   border: 1px solid #EEA634;
   color: #EEA634;
}
.zuo1{
  display: inline-block;
  text-align: center;
  border: 1px solid #72A4B1;
  border-radius: 8rpx;
  background-color: #fff;
  color: #72A4B1;  
margin-left: 20rpx;
}
.ka{
  display: inline-block;
  width: 50rpx;
  text-align: center;
  background-color: #A97AE0;
  border-radius: 8rpx;
  color: #fff;
}
.cuxiao{
  margin-left: 20rpx;
  font-size: 14px;
